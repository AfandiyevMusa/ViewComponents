@model SliderInfoCreateVM


<div class="container my-5">
    <div class="row">
        <form method="post" asp-action="Create" enctype="multipart/form-data">
            <input asp-for="SignImage" class="form-control" />
            <span asp-validation-for="SignImage" class="text-danger d-block"></span>
            <input asp-for="Title" class="form-control mt-3" />
            <span asp-validation-for="Title" class="text-danger d-block"></span>
            <input asp-for="Info" class="form-control mt-3" />
            <span asp-validation-for="Info" class="text-danger d-block"></span>
            <button type="submit" class="btn btn-success btn-rounded btn-fw mt-3 d-inline-block">Submit</button>
            <a asp-controller="Slider" asp-action="Index" class="btn btn-primary btn-rounded btn-fw mt-3 d-inline-block">Back</a>
        </form>
    </div>
</div>